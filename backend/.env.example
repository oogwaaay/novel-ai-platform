# DeepSeek API Key (required for AI generation)
DEEPSEEK_API_KEY=sk-

# OpenAI API Key (optional, fallback if DeepSeek is not available)
OPENAI_API_KEY=sk-

# Database Configuration (Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-service-role-key

# JWT Secret for Authentication
JWT_SECRET=your-strong-jwt-secret

# Session Secret for Express Session
SESSION_SECRET=your-strong-session-secret

# Google OAuth Configuration (optional for Google login)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# GitHub OAuth Configuration (optional for GitHub login)
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Server Configuration
PORT=3000
NODE_ENV=development

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
